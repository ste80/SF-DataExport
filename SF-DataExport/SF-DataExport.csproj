<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <RootNamespace>SF_DataExport</RootNamespace>
    <AssemblyName>sf-dataexport</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetForce" Version="5.0.11" />
    <PackageReference Include="Microsoft.Extensions.CommandLineUtils" Version="1.1.1" />
    <PackageReference Include="PuppeteerSharp" Version="1.11.2" />
    <PackageReference Include="System.Reactive" Version="4.1.3" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="app.html" />
    <EmbeddedResource Include="slds.css" />
    <EmbeddedResource Include="rxjs.js" />
    <EmbeddedResource Include="vue.js" />
    <EmbeddedResource Include="vuex.js" />
    <EmbeddedResource Include="favicon.ico" />
    <EmbeddedResource Include="fonts\**\*.*" />
    <EmbeddedResource Include="icons\**\*.*" />
    <EmbeddedResource Include="images\**\*.*" />
  </ItemGroup>

</Project>
